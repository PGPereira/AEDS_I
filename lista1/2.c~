#include <stdio.h>
#include <math.h>

int main (){
        double a, b, c, d;

        while (scanf("%lf %lf %lf %lf", &a, &b, &c, &d) == 4){
                printf ("a: %lf", a + b);
                printf ("b: %lf", a / c);
                printf ("c: %lf", pow(a, 2));
                printf ("d: %lf", b * c);
                printf ("e: %lf", a * b - c);
                printf ("f: %lf", a + b * c);
                printf ("g: %lf", (a + b) * c);
                printf ("h: %lf", sin(a));
                printf ("i: %lf", sqrt(b));
                printf ("j: %lf", a + b + c);
                printf ("k: %lf", a * b * c);
                printf ("l: %lf", (a + b + c) / d);
                printf ("m: %lf", (a + b) * (a - d));
                printf ("n: %lf", (b / c) * (a * d));
                printf ("o: %lf", sin(b) + cos(c));
                printf ("p: %lf", (b + a) / c - (d +a));
        }

        return 0;
}

